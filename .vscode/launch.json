    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to SAM CLI",
            "type": "node",
            "address":"0.0.0.0",
            "request": "attach",
            "port": 9994,
            "localRoot": "${workspaceFolder}/dist",
            "outFiles": ["${workspaceFolder}/dist/**/*.js"],
            "remoteRoot": "/var/task",
            "protocol": "inspector",
            "stopOnEntry": false,
            "sourceMaps": true
            
          }
    ]
